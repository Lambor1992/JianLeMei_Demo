<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.jlm.app.jianlemei_demo.widget.TitleBar
        android:id="@+id/home_video_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titlename="电台" />
    <com.viewpagerindicator.TabPageIndicator
        android:id="@+id/home_video_tab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone" />

    <android.support.v4.view.ViewPager android:layout_below="@id/home_video_title"
        android:id="@+id/home_video_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <FrameLayout
        android:id="@+id/home_video_fram"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">
        <include layout="@layout/fragment_health__case__eye1" />
    </FrameLayout>
</RelativeLayout>
